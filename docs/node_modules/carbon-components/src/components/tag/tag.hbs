{{#each tags}}
<span class="{{@root.prefix}}--tag {{@root.prefix}}--tag--{{type}}">{{label}}</span>
{{/each}}

{{#if filter}}
<span class="{{@root.prefix}}--tag {{@root.prefix}}--tag--filter" title="Clear filter" tabindex="0">
  filter
  {{ carbon-icon 'Close16' aria-label='Clear filter' }}
</span>
{{/if}}
